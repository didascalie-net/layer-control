class OSCBundle :
    def __init__(self):
	self.message  = struct.pack('8sii', '#bundle', 0, 0)

    def addMess(self, args):
	for mess in args :
		length = len(mess.getBinary())
		self.message = self.message + struct.pack('i%ds' % length, length, mess.getBinary())

