def ipValid(ipAddress):
	if ipAddress == 'localhost' :
		return (True)
	dots = ipAddress.split(".")
	if len(dots) != 4:
		return (False)
	for item in dots:
		if not 0 &lt;= int(item) &lt;= 255:
			return False
	return True

